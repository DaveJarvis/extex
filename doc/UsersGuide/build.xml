<project name="doc-users-guide" default="all" basedir=".">

  <description>ExTeX - build file</description>

  <property name="tmp" location="tmp"/>
  <property name="deploy" location="deploy"/>

  <target name="all"
          description="Build nearly everything">
    <exec executable="make" output="tmp/extex-users.out" failonerror="true"/>
  </target>

  <target name="compile"
          description="Compile the source files">
  </target>

</project>


