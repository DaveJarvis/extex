  <Processor class="org.extex.exbib.core.bst.BstProcessor">
    <globalMax>32000</globalMax>

    <function name="&gt;"
             class="org.extex.exbib.core.bst.code.impl.Gt"/>
    <function name="&lt;"
             class="org.extex.exbib.core.bst.code.impl.Lt"/>
    <function name="="
             class="org.extex.exbib.core.bst.code.impl.Eq"/>
    <function name="+"
             class="org.extex.exbib.core.bst.code.impl.Plus"/>
    <function name="-"
             class="org.extex.exbib.core.bst.code.impl.Minus"/>
    <function name="*"
             class="org.extex.exbib.core.bst.code.impl.Concat"/>
    <function name=":="
             class="org.extex.exbib.core.bst.code.impl.Set"/>
    <function name="add.period$"
             class="org.extex.exbib.core.bst.code.impl.AddPeriod">
      <omitPattern>.*[.!?][}]*$</omitPattern>
    </function>
    <function name="call.type$"
             class="org.extex.exbib.core.bst.code.impl.CallType">
      <default>default.type</default>
    </function>
    <function name="change.case$"
             class="org.extex.exbib.core.bst.code.impl.ChangeCase8"/>
    <function name="chr.to.int$"
             class="org.extex.exbib.core.bst.code.impl.ChrToInt"/>
    <function name="cite$"
             class="org.extex.exbib.core.bst.code.impl.Cite"/>
    <function name="duplicate$"
             class="org.extex.exbib.core.bst.code.impl.Duplicate"/>
    <function name="empty$"
             class="org.extex.exbib.core.bst.code.impl.Empty"/>
    <function name="format.name$"
             class="org.extex.exbib.core.bst.code.impl.FormatName099"/>
    <function name="if$"
             class="org.extex.exbib.core.bst.code.impl.If"/>
    <function name="int.to.chr$"
             class="org.extex.exbib.core.bst.code.impl.IntToChr"/>
    <function name="int.to.str$"
             class="org.extex.exbib.core.bst.code.impl.IntToStr"/>
    <function name="missing$"
             class="org.extex.exbib.core.bst.code.impl.Missing"/>
    <function name="newline$"
             class="org.extex.exbib.core.bst.code.impl.Newline"/>
    <function name="num.names$"
             class="org.extex.exbib.core.bst.code.impl.NumNames"/>
    <function name="pop$"
             class="org.extex.exbib.core.bst.code.impl.Pop"/>
    <function name="preamble$"
             class="org.extex.exbib.core.bst.code.impl.Preamble"/>
    <function name="purify$"
             class="org.extex.exbib.core.bst.code.impl.Purify">
      <map name="ae">ae</map>
      <map name="AE">AE</map>
      <map name="oe">oe</map>
      <map name="OE">OE</map>
      <map name="ss">ss</map>
      <map name="i">i</map>
      <map name="j">j</map>
      <map name="l">l</map>
      <map name="L">L</map>
      <map name="aa">a</map>
      <map name="AA">A</map>
      <map name="o">o</map>
      <map name="O">O</map>
    </function>
    <function name="quote$"
             class="org.extex.exbib.core.bst.code.impl.Quote"/>
    <function name="skip$"
             class="org.extex.exbib.core.bst.code.impl.Skip"/>
    <function name="stack$"
             class="org.extex.exbib.core.bst.code.impl.Stack"/>
    <function name="substring$"
             class="org.extex.exbib.core.bst.code.impl.Substring"/>
    <function name="swap$"
             class="org.extex.exbib.core.bst.code.impl.Swap"/>
    <function name="text.length$"
             class="org.extex.exbib.core.bst.code.impl.TextLength"/>
    <function name="text.prefix$"
             class="org.extex.exbib.core.bst.code.impl.TextPrefix"/>
    <function name="top$"
             class="org.extex.exbib.core.bst.code.impl.Top"/>
    <function name="type$"
             class="org.extex.exbib.core.bst.code.impl.Type"/>
    <function name="warning$"
             class="org.extex.exbib.core.bst.code.impl.Warning"/>
    <function name="while$"
             class="org.extex.exbib.core.bst.code.impl.While"/>
    <function name="width$"
             class="org.extex.exbib.core.bst.code.impl.Width"/>
    <function name="write$"
             class="org.extex.exbib.core.bst.code.impl.Write"/>
  </Processor>
