<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <artifactId>ExTeX-base-ext</artifactId>
  <name>ExTeX Base Ext</name>
  <version>0.1-SNAPSHOT</version>
  <description>
    This is an ExTeX extended base component.
  </description>

  <packaging>jar</packaging>
  <inceptionYear>2003</inceptionYear>
  <url>http://www.extex.org</url>

  <parent>
    <groupId>org.extex</groupId>
    <artifactId>extex</artifactId>
    <version>0.1-SNAPSHOT</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <repositories>
    <repository>
      <id>extexSnapshots</id>
      <name>ExTeX Snapshots</name>
      <url>http://www.extex.org/maven2</url>
      <layout>default</layout>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>

  <dependencies>
    <dependency>
      <groupId>net.sourceforge.jeval</groupId>
      <artifactId>jeval</artifactId>
      <version>0.9.4</version>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-core</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-base</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Font-API</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Typesetter-API</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Interpreter-API</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Interpreter-max</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Scanner-API</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Scanner</artifactId>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Typesetter-core</artifactId>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Backend-dump</artifactId>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Unit-tex</artifactId>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Unit-native</artifactId>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-Backend-dvix</artifactId>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>org.extex</groupId>
      <artifactId>ExTeX-base-test</artifactId>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <developers>
    <developer>
      <id>mgn</id>
      <name>Michael Niedermair</name>
      <email>m.g.n@gmx.de</email>
      <roles>
        <role>maintainer</role>
        <role>developer</role>
      </roles>
      <timezone>+1</timezone>
      <properties/>
    </developer>
  </developers>

</project>
