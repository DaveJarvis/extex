<?xml version="1.0"?>
<ExTeX >

<Resource>
  <Finder class="org.extex.util.resource.FileFinder"
          default="default">
    <u2t>
      <extension>.u2t</extension>
      <path>../ExTeX-Font/src/font</path>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </u2t>
    <tex>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <path>../texmf/src/texmf/tex/plain/base</path>
      <extension></extension>
      <extension>.tex</extension>
    </tex>
    <fmt>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension>.fmt</extension>
    </fmt>
    <efm>
      <extension>.efm</extension>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </efm>
    <pfb>
      <extension>.pfb</extension>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </pfb>
    <pfa>
      <extension>.pfa</extension>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </pfa>
    <afm>
      <extension>.afm</extension>
      <path>../ExTeX-Font/src/font</path>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </afm>
    <ttf>
      <extension>.ttf</extension>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </ttf>
    <vf>
      <extension>.vf</extension>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </vf>
    <tfm>
      <extension>.tfm</extension>
      <path>../texmf/src/texmf/fonts/tfm/public/cm</path>
      <path>../texmf/src/texmf/fonts/tfm/public/latex</path>
      <path>../texmf/src/texmf/fonts/tfm/public/misc</path>
      <path>../ExTeX-Font/src/font</path>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </tfm>
    <enc>
      <extension>.enc</extension>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </enc>
    <map>
      <extension>.enc</extension>
      <path>../ExTeX-Font/src/font</path>
      <path>.</path>
      <path property="extex.fonts"/>
      <path property="texinputs"/>
    </map>
    <default>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path property="extex.fonts"/>
      <path>../ExTeX-Font/src/font</path>
      <path>.</path>
      <extension></extension>
    </default>
  </Finder>

</Resource>

  <Fonts class="org.extex.font.FontFactoryImpl">
		<Font type="tfm" class="org.extex.font.format.tfm.LoadableTfmFont" />
		<Font type="afm" class="org.extex.font.format.afm.LoadableAfmFont" />
<!--      <type1 class="org.extex.font.type.efm.EFMType1AFMFount"/>        
      <ttf class="org.extex.font.type.efm.EFMTTFFount"/>        
      <tfm-mathsyml class="org.extex.font.type.efm.EFMType1TFMMathsymlFount"/>        
      <tfm-mathext class="org.extex.font.type.efm.EFMType1TFMMathextFount"/>
      <tfm-normal class="org.extex.font.type.efm.EFMType1TFMNOFount"/>
      -->
  </Fonts>
  
</ExTeX>
  